{
   "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "importsNotUsedAsValues": "remove",
      "strict": true,
      "skipLibCheck": true
   },
   "include": ["src/**/*", "src/types.d.ts"],
   "exclude": ["node_modules"]
}
